{
  "name": "YouTube Channel Data",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.tsx"
      ],
      "run_at": "document_end"
    }
  ]
}